<style>
    body {
        position: relative;
    }

    #sky {
        min-height: 200vh;
        width: 100%;
        position: fixed;
        bottom: 0;
        left: 0;
        background:
            radial-gradient(
                circle at 75% 24%,
                rgba(255, 255, 255, 0.25) 0.25%,
                transparent 0.5%
            ),
            radial-gradient(
                circle at 10% 23%,
                rgba(255, 255, 255, 0.6) 0.25%,
                transparent 0.25%
            ),
            radial-gradient(
                circle at 98% 29%,
                rgba(255, 255, 255, 0.25) 0.25%,
                transparent 0.25%
            ),
            radial-gradient(
                circle at 63% 10%,
                rgba(255, 255, 255, 0.6) 0.25%,
                transparent 0.25%
            ),
            radial-gradient(
                circle at 20% 6%,
                rgba(255, 255, 255, 0.25) 0.25%,
                transparent 0.25%
            ),
            radial-gradient(
                circle at 75% 5%,
                rgba(255, 255, 255, 0.6) 0.25%,
                transparent 0.25%
            ),
            radial-gradient(
                circle at 35% 15%,
                rgba(255, 255, 255, 0.25) 0.25%,
                transparent 0.25%
            ),
            radial-gradient(
                circle at 94% 10%,
                rgba(255, 255, 255, 0.25) 0.25%,
                transparent 0.25%
            ),
            radial-gradient(
                circle at 55% 30%,
                rgba(255, 255, 255, 0.25) 0.25%,
                transparent 0.25%
            ),
            radial-gradient(
                circle at 4% 40%,
                rgba(255, 255, 255, 0.25) 0.25%,
                transparent 0.25%
            ),
            url(),
                linear-gradient(to top, #6478c1, #010011 60%);
        z-index: -1;
    }

    main {
        display: flex;
        flex-direction: column;
        gap: 20em;
        margin-top: 100vh;
    }

    section {
        opacity: 0;
        height: 150vh;
        /* background-color: rgba(0, 0, 0, 0.1); */
        position: relative;
    }

    main section .graph {
        position: sticky;
        top: 50%;
        transform: translateY(-50%);

        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
    }

    .rocket__container {
        position: sticky;
        top: 0;
        left: 0;
        width: 100%;
        height: 100dvh;
        z-index: 99;
        pointer-events: none;
        overflow: hidden;
    }

    #earth{
        position: absolute;
        bottom: 0;
        width: 100%;
        height: 150px;
        left: 0;
        z-index: -1;
    }

    #rocket {
        height: 100%;
        width: 100%;
        fill: #fff;
        margin: 0 auto;
    }

    #rocket .hidden {
        display: none;
        height: 0;
    }

    .tothemoon {
        animation: tothemoon 2s linear forwards;
        animation-delay: 0.1s;
    }

    .backtoearth {
        animation: toearth 2s linear forwards;
    }

    .loosemodule {
        animation: loosemodule 3s linear forwards;
    }

    @keyframes tothemoon {
        0% {
            transform: translateY(0);
        }
        100% {
            transform: translateY(-100vh);
        }
    }

    @keyframes toearth {
        0% {
            transform: translateY(-100vh);
        }
        100% {
            transform: translateY(0);
        }
    }

    @keyframes loosemodule {
        0% {
            transform: translateY(0);
        }
        100% {
            transform: translateY(100vh) translateX(100vw) rotate(20deg);
        }
    }

</style>

<script>
    import { gsap } from "gsap";
    import { ScrollTrigger } from "gsap/ScrollTrigger";
    gsap.registerPlugin(ScrollTrigger);

    const sky = document.querySelector("#sky") as HTMLElement;
    let bottom = 0;

    window.addEventListener("scroll", (e) => {
        let h = window.innerHeight;
        let y = document.documentElement.scrollTop;
        let doc = document.body.offsetHeight - 250;
        let perc = y / (doc - h);

        if (perc < 1) {
            sky.style.bottom = -1 * perc * 100 + "%";
        }

        if (perc > 0.25) {
            bottom = (perc - 0.25) * 133;
        }

        if (perc > 0) {
            bottom = (perc - 0.25) * 133;
            if (perc - 0.25 < 0) {
                bottom = 0;
            }
        }
    });

    //stages
    const stage1 = document.querySelector("#stage1") as HTMLElement;
    const stage2 = document.querySelector("#stage2") as HTMLElement;
    const stage3 = document.querySelector("#stage3") as HTMLElement;
    const stage4 = document.querySelector("#stage4") as HTMLElement;
    const stages = [stage1, stage2, stage3, stage4];
    const sections = document.querySelectorAll("main section");

    const tl = gsap.timeline();

    //faire partir la terre
    tl.to('#earth', {
        y: 150,
        scrollTrigger: {
            trigger: "#earth",
            start: "-200% center",
            end: "200% center",
            scrub: true,
            // markers: true,
        },
    });

    sections.forEach((section, i) => {
        const stage = stages[i];
        const otherStages = stages.filter((_, j) => j !== i);
        
        //détacher les étages
        tl.to(stage, {
            y: "150%",
            x: "100%",
            opacity: 0,
            rotation: 20,
            scrollTrigger: {
                trigger: section,
                start: "top 60%",
                end: "0 10%",
                scrub: true,
                // markers: true,
            },
        });

        //faire monter les autres étages
        otherStages.forEach(otherStage => {
            tl.to(otherStage, {
                y: "-70vh",
                scrollTrigger: {
                    trigger: section,
                    start: "1% 60%",
                    end: "0 10%",
                    scrub: true,
                    // markers: true,
                },
            });
        });

        //faire apparaitre les graphs
        tl.to(section, {
            opacity: 0,
            scrollTrigger: {
                trigger: section,
                start: "top center",
                end: "30% center",
                // scrub: true,
                // markers: true,
                onEnter: () => {
                    gsap.to(section, {
                        opacity: 1,
                        duration: 1
                    });
                },
                onLeave: () => {
                    gsap.to(section, {
                        opacity: 0,
                        duration: 1
                    });
                    otherStages.forEach(otherStage => {
                        gsap.to(otherStage, {
                            y: "0",
                            duration: 1
                        });
                    });
                },
                onEnterBack: () => {
                    gsap.to(section, {
                        opacity: 1,
                        duration: 1
                    });
                },
                onLeaveBack: () => {
                    gsap.to(section, {
                        opacity: 0,
                        duration: 1
                    });
                    otherStages.forEach(otherStage => {
                        gsap.to(otherStage, {
                            y: "0",
                            duration: 1
                        });
                    });
                }
            },
        });
    });



</script>
